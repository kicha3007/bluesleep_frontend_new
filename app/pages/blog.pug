extend include/template_base


block variables
	- var title = 'Блог';

block content
	.blog
		.blog__container.container
			.blog__wrap
				each item in blogList
					a.blog__item(href="#")
						.blog__inner-wrap
							.blog__img-box
								img.blog__img(src=item.img, alt='')
							.blog__title= item.title
							.blog__desc= item.desc
							.blog__date= item.date
