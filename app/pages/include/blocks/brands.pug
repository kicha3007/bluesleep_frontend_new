mixin brands(data, mods)
	section.brands&attributes(attributes)
		.brands__container.container
			.brands__wrap.js-brands-wrap
				each item in $brandList
					- mod = typeof item.mod !== "undefined" && item.mod ? "brands__img-box_" + item.mod : ""
					a.brands__item(href="#")
						.brands__img-box(class=mod)
							.brands__inner-img-box
								img.brands__img(src=item.img.hover, alt='')
								img.brands__img.brands__img_hov(src=item.img.normal, alt='')
